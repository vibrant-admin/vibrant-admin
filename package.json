{
  "name": "vibrant-admin",
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "run-p lint \"build-only {@}\" --",
    "preview": "vite preview",
    "build-only": "vite build",
    "lint": "run-p lint:tsc lint:eslint lint:stylelint",
    "lint:tsc": "vue-tsc --build",
    "lint:eslint": "eslint . --fix",
    "lint:stylelint": "stylelint \"src/**/*.{css,vue}\" --fix",
    "up": "taze major -I",
    "preinstall": "npx only-allow pnpm",
    "postinstall": "simple-git-hooks",
    "cz": "czg"
  },
  "dependencies": {
    "@vueuse/core": "^13.5.0",
    "clsx": "^2.1.1",
    "element-plus": "^2.10.4",
    "es-toolkit": "^1.39.7",
    "pinia": "^3.0.3",
    "tailwind-merge": "^3.3.1",
    "vue": "^3.5.18",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.18.0",
    "@iconify/json": "^2.2.362",
    "@stylistic/stylelint-config": "^3.0.0",
    "@tsconfig/node22": "^22.0.2",
    "@types/node": "^24.1.0",
    "@unocss/core": "^66.3.3",
    "@unocss/eslint-plugin": "^66.3.3",
    "@vitejs/plugin-legacy": "^7.1.0",
    "@vitejs/plugin-vue": "^6.0.0",
    "@vitejs/plugin-vue-jsx": "^5.0.1",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/tsconfig": "^0.7.0",
    "autoprefixer": "^10.4.21",
    "czg": "^1.12.0",
    "eslint": "^9.31.0",
    "eslint-plugin-format": "^1.0.1",
    "lint-staged": "^16.1.2",
    "npm-run-all2": "^8.0.4",
    "path-to-regexp": "^8.2.0",
    "postcss": "^8.5.6",
    "simple-git-hooks": "^2.13.0",
    "stylelint": "^16.22.0",
    "stylelint-config-recess-order": "^7.1.0",
    "stylelint-config-recommended-vue": "^1.6.1",
    "taze": "^19.1.0",
    "typescript": "~5.8.3",
    "unocss": "^66.3.3",
    "unplugin-auto-import": "^19.3.0",
    "unplugin-vue-components": "^28.8.0",
    "vite": "^7.0.5",
    "vite-plugin-app-loading": "^0.4.0",
    "vite-plugin-compression2": "^2.2.0",
    "vite-plugin-env-parse": "^1.0.15",
    "vite-plugin-vue-devtools": "^7.7.7",
    "vite-svg-loader": "^5.1.0",
    "vue-tsc": "^3.0.3"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged",
    "preserveUnused": true
  }
}
